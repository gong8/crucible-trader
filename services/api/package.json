{
  "name": "@crucible-trader/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "dev": "ts-node --esm src/index.ts",
    "start": "node dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src": "./src/index.ts",
    "./queue": {
      "types": "./dist/queue.d.ts",
      "import": "./dist/queue.js",
      "default": "./dist/queue.js"
    }
  },
  "dependencies": {
    "@crucible-trader/sdk": "workspace:*",
    "fastify": "^5.6.1"
  },
  "typesVersions": {
    "*": {
      "queue": ["src/queue.ts"]
    }
  }
}
